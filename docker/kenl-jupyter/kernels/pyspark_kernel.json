{
    "display_name": "PySpark_Python3",
    "language": "python",
    "argv": [
        "/usr/bin/python3",
        "-m",
        "ipykernel_launcher",
        "-f",
        "{connection_file}"
    ],
    "env": {
        "SPARK_HOME": "/opt/kenl/spark/",
        "PYTHONPATH": "/opt/kenl/spark/python/:/opt/kenl/spark/python/lib/py4j-0.10.7-src.zip",
        "PYTHONSTARTUP": "/opt/kenl/spark/python/pyspark/shell.py",
        "PYSPARK_PYTHON": "/usr/bin/python3"
    }
}